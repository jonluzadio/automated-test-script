{
  "name": "automated-test-script",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "open": "cypress open",
    "all-test": "cypress run --browser chrome --headed",
    "generate-report": "node generate-report.js",
    "open-report": "start cypress/reports/cucumber-html/index.html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^24.0.0",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.8",
    "cypress": "^15.9.0",
    "multiple-cucumber-html-reporter": "^3.9.3"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": true,
    "stepDefinitions": "cypress/e2e/step_definitions/**/*.{js,ts}",
    "json": {
      "enabled": true,
      "output": "cypress/reports/cucumber-json/[filepath].cucumber.json",
      "formater": "node_modules/@badeball/cypress-cucumber-preprocessor/json-report-formatter"
    }
  }
}
